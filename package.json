{
  "name": "@trim21/gm-fetch",
  "version": "0.1.13",
  "description": "fetch for UserScript",
  "keywords": [
    "userscript",
    "typescript"
  ],
  "prettier": {
    "printWidth": 120,
    "trailingComma": "all"
  },
  "packageManager": "yarn@3.5.0",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "./dist/types/src/index.d.ts",
  "jsdelivr": "dist/gm_fetch.js",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "./src/",
    "./dist/"
  ],
  "scripts": {
    "format": "prettier --list-different -w ./",
    "version": "npm run build",
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "test": "tsc --noEmit && jest",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,yml,yaml}": [
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trim21/gm-fetch.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Trim21/gm-fetch/issues"
  },
  "homepage": "https://github.com/Trim21/gm-fetch",
  "devDependencies": {
    "@types/greasemonkey": "^4.0.4",
    "@types/jest": "^29.5.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.3",
    "unbuild": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
